; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "MapGen",
"language": "GDScript",
"path": "res://src/utils/map_generation.gd"
}, {
"base": "Node",
"class": "Utils",
"language": "GDScript",
"path": "res://src/utils/utils.gd"
} ]
_global_script_class_icons={
"MapGen": "",
"Utils": ""
}

[application]

config/name="Esper"
config/description="CO-OP 2D roguelike dungeon crawler themed around a 6th sense!"
run/main_scene="res://src/main_menu/main_menu.tscn"
config/use_custom_user_dir=true
boot_splash/bg_color=Color( 0.141176, 0.141176, 0.141176, 1 )
config/icon="res://icon.png"

[autoload]

Gamestate="*res://src/utils/gamestate.tscn"
Shortlivedconfig="*res://src/utils/shortlived_config.gd"
SoundManager="*res://src/utils/soundmanager.tscn"

[display]

window/size/width=256
window/size/height=150
window/size/test_width=1024
window/size/test_height=600
window/stretch/mode="2d"
window/stretch/aspect="keep"

[gui]

common/drop_mouse_on_gui_input_disabled=true

